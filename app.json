{
  "expo": {
    "name": "Reise Budget",
    "slug": "vacation-assist",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vacationassist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Verwalte dein Reisebudget einfach und effizient. Erfasse Ausgaben, plane dein Budget und behalte den Überblick über deine Reisekosten.",
    "primaryColor": "#007AFF",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vomschoeni.ferien-budget",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationCategoryType": "public.app-category.travel",
        "CFBundleLocalizations": ["de", "en", "fr", "it"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.vomschoeni.ferienbudget"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#007AFF",
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Die App benötigt Zugriff auf deine Fotos, um Reisebilder hinzuzufügen.",
          "cameraPermission": "Die App benötigt Zugriff auf deine Kamera, um Reisefotos aufzunehmen."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ebfbcf49-c303-4b8c-bcfb-80a723f96d0e"
      }
    },
    "owner": "cschoeni",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ebfbcf49-c303-4b8c-bcfb-80a723f96d0e"
    }
  }
}
